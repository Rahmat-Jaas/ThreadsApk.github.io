<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:kf="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="@dimen/clips_controls_with_attached_scrubber_hairline_height" android:thickness="@dimen/audition_audio_item_selected_stroke_border_width">
            <solid android:color="@color/cds_white_a20"/>
            <size android:height="@dimen/avatar_reel_ring_size_extra_large" android:width="@dimen/avatar_reel_ring_size_extra_large"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="0" android:toDegrees="1080" android:pivotX="50%" android:pivotY="50%" android:repeatCount="infinite">
            <shape android:shape="ring" android:useLevel="false" android:innerRadius="@dimen/clips_controls_with_attached_scrubber_hairline_height" android:thickness="@dimen/audition_audio_item_selected_stroke_border_width">
                <gradient android:startColor="@color/fds_transparent" android:endColor="@color/design_dark_default_color_on_background" android:angle="0" android:type="sweep"/>
                <size android:height="@dimen/avatar_reel_ring_size_extra_large" android:width="@dimen/avatar_reel_ring_size_extra_large"/>
            </shape>
        </rotate>
    </item>
</layer-list>

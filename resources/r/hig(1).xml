<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:kf="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/seek_bar_knob_outer_circle">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="10dp" android:thickness="@dimen/audition_audio_item_selected_stroke_border_width">
            <solid android:color="@color/feed_sticker_background_top_light"/>
        </shape>
    </item>
    <item android:id="@+id/seek_bar_knob_inner_circle" android:left="11dp" android:top="11dp" android:right="11dp" android:bottom="11dp">
        <shape android:shape="oval">
            <solid android:color="@color/feed_sticker_background_top_light"/>
        </shape>
    </item>
</layer-list>

<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:kf="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/message_reactions_pill_shadow_layer">
        <shape android:shape="rectangle">
            <padding android:left="@dimen/abc_control_corner_material" android:top="@dimen/abc_control_corner_material" android:right="@dimen/abc_control_corner_material" android:bottom="@dimen/abc_control_corner_material"/>
            <corners android:radius="@dimen/account_permission_section_vertical_padding"/>
        </shape>
    </item>
    <item android:id="@+id/message_reactions_pill_background_layer">
        <shape android:shape="rectangle">
            <padding android:top="@dimen/abc_control_corner_material" android:bottom="@dimen/abc_control_corner_material"/>
            <corners android:radius="@dimen/account_section_text_margin_horizontal"/>
        </shape>
    </item>
</layer-list>
